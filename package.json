{
  "name": "code-crawler",
  "displayName": "code-crawler",
  "description": "An awesome extension to freely navigate through your code without using mouse and much effort.",
  "version": "1.0.0",
  "publisher": "DhananjayChauhan",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dhananjay-Chauhan/code-crawler.git"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-crawler.navigateNext",
        "title": "Hello World"
      },
      {
        "command": "code-crawler.navigatePrevious",
        "title": "Hello Worlds"
      },
      {
        "command": "code-crawler.nextError",
        "title": "Hello Error"
      },
      {
        "command": "code-crawler.prevError",
        "title": "Bye Error"
      },
      {
        "command": "code-crawler.jumpToMatchingBracket",
        "title": "Hello Bracket"
      },
      {
        "command": "code-crawler.selectBetweenBrackets",
        "title": "Hello inBracket"
      }
    ],
    "keybindings": [
      {
        "command": "code-crawler.navigateNext",
        "key": "ctrl+shift+]",
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.navigatePrevious",
        "key": "ctrl+shift+[",
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.nextError",
        "key": "ctrl+shift+1",
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.prevError",
        "key": "ctrl+shift+2",
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.jumpToMatchingBracket",
        "key": "ctrl+shift+3",
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.selectBetweenBrackets",
        "key": "ctrl+shift+4",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "vscode:prepublish": "pnpm run package",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
