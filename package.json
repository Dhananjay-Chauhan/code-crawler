{
  "name": "code-crawler",
  "displayName": "code-crawler",
  "description": "An awesome extension to freely navigate  through your code  without using mouse  and much effort.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-crawler.navigateNext",
        "title": "Hello World"
      },
      {
        "command": "code-crawler.navigatePrevious",
        "title": "Hello Worlds"
      },
      {
        "command": "code-crawler.nextError",
        "title": "Hello Error"
      },
      {
        "command": "code-crawler.prevError",
        "title": "Bye Error"
      },
      {
        "command": "code-crawler.jumpToMatchingBracket",
        "title": "Hello Bracket"
      },
      {
        "command": "code-crawler.selectBetweenBrackets",
        "title": "Hello inBracket"
      }
    ],
    "keybindings": [
      {
        "command": "code-crawler.navigateNext",
        "key": "ctrl+shift+]", 
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.navigatePrevious",
        "key": "ctrl+shift+[", 
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.nextError",
        "key": "ctrl+shift+1", 
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.prevError",
        "key": "ctrl+shift+2", 
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.jumpToMatchingBracket",
        "key": "ctrl+shift+3", 
        "when": "editorTextFocus"
      },
      {
        "command": "code-crawler.selectBetweenBrackets",
        "key": "ctrl+shift+4", 
        "when": "editorTextFocus"
      }

    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
